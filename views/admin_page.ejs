        <td>조회수</td>
        <td>수정일</td>
        <td>등록일</td>
    </tr>
    <%
    for(var i=(page*page_num)-page_num; i<(page*page_num) ; i++){
        if(i>length){
            i++;
        }else{
            var data = rows[i];
    %>
    <tr>
        <td><%= data.name %></td>
        <td><a href="/admin/read/<%= data._id %>"><%= data.title %></a></td>
        <td><%= data.hit %></td>
        <td><%= data.modidate %></td>
        <td><%= data.regdate %></td>
    </tr>
    <%
        }
    }
    %>
    <tr>
        <td colspan="6">
            <%
                for(var j=0 ; j<rows.length / page_num ; j++){
            %>
                    [<a href="/admin/page/<%= j+1 %>"><%= j+1 %></a>]
            <%
                }
            %>
        </td>
    </tr>
</table>

</body>
</html>